<?xml version="1.0" ?>
<tag name="iManga">
    <tag name="chapters">
        <dbloop name="chapter" tables="fs_chapters" filters="">
            <tag name="chapter">
                <attribute name="id" value="dbvalue(chapter, fs_chapters.uniqid)" />
                <attribute name="created" value="strtotime(dbvalue(chapter, fs_chapters.created))" />
                <attribute name="updated" value="strtotime(dbvalue(chapter, fs_chapters.updated))" />
                <value value="concat(param(site.uri.uri), ?chapter=, dbvalue(chapter, fs_chapters.uniqid))" />
            </tag>
        </dbloop>
    </tag>
</tag>

