<?xml version="1.0" ?>
<tag name="iManga">
    <tag name="mangas">
        <dbloop name="manga" tables="fs_comics" filters="">
            <tag name="manga">
                <attribute name="id" value="dbvalue(manga, fs_comics.uniqid)" />
                <attribute name="created" value="strtotime(dbvalue(manga, fs_comics.created))" />
                <attribute name="updated" value="strtotime(dbvalue(manga, fs_comics.updated))" />
                <value value="concat(param(site.uri.uri), ?manga=, dbvalue(manga, fs_comics.uniqid))" />
            </tag>
        </dbloop>
    </tag>
</tag>
